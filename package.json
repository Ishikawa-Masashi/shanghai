    "lint": "eslint --fix \"src/**/*.{js,ts}\"",
    "copy-linux": "mkdir -p ./dist/assets/assets && cp -pR ./assets/* ./dist/assets/assets",
    "all": "run-s build copy-linux"
  },
  "devDependencies": {
    "@atsumaru/api-types": "atsumaru/api-types#master",
    "@types/howler": "^2.2.7",
