    "@ishikawa_masashi/react-hooks": "^1.2.5",
    "disable-scroll": "^0.6.0",
    "firebase": "^9.13.0",
    "framer-motion": "^7.6.5",
    "howler": "^2.2.3",
    "p5": "^1.5.0",
