  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist types",
    "watch": "tsc -w | vite build --watch",
    "build": "yarn clean && tsc && vite build",
    "build:lib": "tsc && vite build --config vite.config.lib.ts",
